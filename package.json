{
	"name": "@toriyama/gyolets",
	"description": "行列簡約化ツール",
	"version": "0.9.7",
	"main": "dist/main.js",
	"bin": "dist/bin.js",
	"scripts": {
		"build": "rm -rf ./dist && npm run clean && tsc",
		"build:watch": "tsc --watch",
		"build:docs": "typedoc --out public/docs",
		"build:web": "rm -rf ./public && tsc --noEmit && parcel build web/index.html -d public",
		"build:web:watch": "parcel web/index.html --open",
		"test": "jest",
		"test:utils": "jest test/utils.test.ts",
		"test:main": "jest test/main.test.ts",
		"prepublishOnly": "npm run test && npm run build && npm run build:docs && npm run build:web"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/YUUKIToriyama/gyolets.git"
	},
	"author": "YUUKIToriyama <github@toriyama.dev>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/YUUKIToriyama/gyolets/issues"
	},
	"homepage": "https://github.com/YUUKIToriyama/gyolets#readme",
	"devDependencies": {
		"@types/jest": "^26.0.23",
		"@types/node": "^15.9.0",
		"@types/react": "^17.0.11",
		"@types/react-dom": "^17.0.8",
		"jest": "^27.0.4",
		"parcel-bundler": "^1.12.5",
		"ts-jest": "^27.0.2",
		"typedoc": "^0.20.36",
		"typescript": "^4.2.4"
	},
	"dependencies": {
		"@blueprintjs/core": "^3.45.0",
		"cac": "^6.7.3",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"jest": {
		"preset": "ts-jest"
	}
}
